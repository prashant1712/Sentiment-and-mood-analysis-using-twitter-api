        <servlet-name>graph1</servlet-name>
        <url-pattern>/graph1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>favourite1</servlet-name>
        <url-pattern>/favourite1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>finaluser</servlet-name>
        <url-pattern>/finaluser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
