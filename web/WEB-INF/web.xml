        <servlet-class>MyProject.graph</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>graph1</servlet-name>
        <servlet-class>MyProject.graph1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>favourite1</servlet-name>
        <servlet-class>MyProject.favourite1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>finaluser</servlet-name>
